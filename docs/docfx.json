{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "code-output/**/**.dll"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/*.yml",
          "articles/**.md",
          "articles/**/toc.yml",
          "developer-info/**.md",
          "developer-info/toc.yml",
          "developer-info/Packaging/toc.yml",
          "developer-info/Packaging/**.md",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": "**/**",
        "src": "legacy",
        "dest": ""
      },
      {
        "files": [
          "CNAME",
          "images/**",
          "favicons/**",
          "styles/*.js",
          "css/**",
          "js/**",
          "img/**",
          "robots.txt",
          "articles/vs-test-adapter/Adapter-Release-Notes.html"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "NUnit Docs",
      "_googleAnalyticsTagId": "G-PCB5BYBNM2",
      "_appFooter": "<span>Generated by <strong>DocFX</strong></span> | Copyright (c) 2018-<span id=\"currentYear\"></span> The NUnit Project - Licensed under <a href=\"https://creativecommons.org/licenses/by-nc-sa/4.0/\" target=\"_blank\">CC BY-NC-SA 4.0</a>",
      "_lang": "en"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "custom_template"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "sitemap": {
      "baseurl": "http://docs.nunit.org"
    }
  }
}